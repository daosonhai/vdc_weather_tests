{
    "baseUrl": "https://openweathermap.org",
    "defaultCommandTimeout": 4000,
    "fixturesFolder": "cypress/Datasets",
    "integrationFolder": "cypress/Tests",
    "screenshotsFolder": "cypress/Attachment/Screenshots",
    "supportFile": "cypress/Actions",
    "video": true,
    "videosFolder": "cypress/Attachment/Videos",
    "videoUploadOnPasses": false,
    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "configFile": "cypress/Reports/ReportConfig/ReporterOptions.json"
    },
    "testFiles": "**/*.feature",
    "env": {
        "baseUrlApi": "https://openweathermap.org"
    }
}